<p id="notice"><%= notice %></p>

<h1>Listing Post Favs</h1>

<table>
  <thead>
    <tr>
      <th>Post</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @post_favs.each do |post_fav| %>
      <tr>
        <td><%= post_fav.post %></td>
        <td><%= post_fav.user %></td>
        <td><%= link_to 'Show', post_fav %></td>
        <td><%= link_to 'Edit', edit_post_fav_path(post_fav) %></td>
        <td><%= link_to 'Destroy', post_fav, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post fav', new_post_fav_path %>
