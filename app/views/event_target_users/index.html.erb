<p id="notice"><%= notice %></p>

<h1>Listing Event Target Users</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_target_users.each do |event_target_user| %>
      <tr>
        <td><%= event_target_user.event %></td>
        <td><%= event_target_user.user %></td>
        <td><%= link_to 'Show', event_target_user %></td>
        <td><%= link_to 'Edit', edit_event_target_user_path(event_target_user) %></td>
        <td><%= link_to 'Destroy', event_target_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

