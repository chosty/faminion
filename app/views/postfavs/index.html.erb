<p id="notice"><%= notice %></p>

<h1>Listing Postfavs</h1>

<table>
  <thead>
    <tr>
      <th>Post</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @postfavs.each do |postfav| %>
      <tr>
        <td><%= postfav.post %></td>
        <td><%= postfav.user %></td>
        <td><%= link_to 'Show', postfav %></td>
        <td><%= link_to 'Edit', edit_postfav_path(postfav) %></td>
        <td><%= link_to 'Destroy', postfav, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Postfav', new_postfav_path %>
